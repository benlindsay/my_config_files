# This is Git's per-user configuration file.
[user]
	email = benjlindsay@gmail.com
	name = Ben Lindsay
[push]
	default = simple
	followTags = true
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[alias]
	unstage = reset HEAD --
	st = status
	co = checkout
	br = branch
	last = log -1 HEAD
	cm = commit
	graph = log --graph --oneline --decorate --all
	pom = push origin master
